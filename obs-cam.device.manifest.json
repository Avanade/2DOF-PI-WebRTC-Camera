{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "registryName": {
                "address": "",
                "password": "",
                "username": ""
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.4.9",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.4.9",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"443/tcp\":[{\"HostPort\":\"443\"}],\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}]}}}"
            }
          }
        },
        "modules": {
          "device": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "innovationcontainerimages.azurecr.io/device:telemetry-1.0.664",
              "createOptions": "{\"NetworkingConfig\":{\"EndpointsConfig\": {\"host\": {}}},HostConfig:{\"Privileged\": true,\"NetworkMode\": \"host\",\"LogConfig\": {\"Type\": \"json-file\",\"Config\": {\"max-file\": \"5\",\"max-size\": \"10m\"}}}}"
            },
            "env": {}
          },
          "camera": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "innovationcontainerimages.azurecr.io/obs-cam:streamer",
              "createOptions": "{\"NetworkingConfig\":{\"EndpointsConfig\": {\"host\": {}}},\"HostConfig\":{\"Privileged\": true, \"GroupAdd\": [\"video\"], \"Binds\": [\"/run/udev:/run/udev\"] ,\"NetworkMode\": \"host\",\"LogConfig\": {\"Type\": \"json-file\",\"Config\": {\"max-file\": \"5\",\"max-size\": \"10m\"}},\"Devices\": [{\"PathOnHost\": \"/dev/vchiq\",\"PathInContainer\": \"/dev/vchiq\",\"CgroupPermissions\": \"rwm\"}]}}"
            },
            "env": {
              "USE_AZ_IOT": {
                "value": "TRUE"
              }
            }
          },
          "controller": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "innovationcontainerimages.azurecr.io/obs-cam:controller",
              "createOptions": "{\"HostConfig\":{\"Privileged\": true,\"LogConfig\": {\"Type\": \"json-file\",\"Config\": {\"max-file\": \"5\",\"max-size\": \"10m\"}},\"Devices\": [{\"PathOnHost\": \"/dev/i2c-1\",\"PathInContainer\": \"/dev/i2c-1\",\"CgroupPermissions\": \"rwm\"}]}}"
            },
            "env": {}
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "route": "FROM /* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "device": {
      "properties.desired": {
        "logging_level": "INFO",
        "az_logging_level": "WARNING",
        "period": 10
      }
    },
    "camera": {
      "properties.desired": {
        "logging_level": "INFO",
        "az_logging_level": "WARNING",
        "period": 60,
        "max_clients": 5,
        "monitoring_period": 60,
        "check_client_heartbeat": true,
        "missed_heartbeats_allowed": 3,
        "force_client_disconnect_duration": 0,
        "cam_vendor_id": "0x0edc",
        "cam_product_id": "0x2050",
        "settings": {
          "stream_id": "htxi-robot",
          "server": "wss://wss.vdo.ninja:443",
          "peer_url_base": "https://vdo.ninja/alpha",
          "stun_server": "stun://stun4.l.google.com:19302",
          "cam_source": "v4l2src",
          "cam_source_params": "device=/dev/video0",
          "custom_pipeline": "videoflip method=vertical-flip !",
          "fps": 10,
          "width": 1280,
          "height": 720,
          "caps": "video/x-raw,width={width},height={height},framerate={fps}/1"
        }
      }
    },
    "controller": {
      "properties.desired": {
        "environment": {
          "controller": {
            "i2c_bus": 1,
            "address": 64,
            "frequency": 26500000,
            "resolution": 4096,
            "servo_frequency": 50,
            "logging_level": "WARNING",
            "type": "PCA9865"
          },
          "servos": {
            "logging_level": "WARNING",
            "angle_increment": 0.05,
            "base": {
              "channel": 15,
              "type": "custom",
              "attributes": {
                "pulse": {
                  "min": 0.55,
                  "max": 2.35,
                  "neutral": 1.4
                },
                "angle": {
                  "min": -85.0,
                  "max": 85.0,
                  "neutral": -0.0
                }
              },
              "range": {
                "neutral": -0.0,
                "max": 84,
                "min": -84
              },
              "trim": 0.0
            },
            "elevation": {
              "channel": 14,
              "type": "custom",
              "attributes": {
                "pulse": {
                  "min": 0.55,
                  "max": 2.35,
                  "neutral": 1.4
                },
                "angle": {
                  "min": -85.0,
                  "max": 85.0,
                  "neutral": -0.0
                }
              },
              "range": {
                "neutral": 68,
                "max": 84.9,
                "min": 25
              },
              "trim": 0.0
            },
            "zoom": {
              "channel": -1,
              "type": "None",
              "attributes": {
                "pulse": {
                  "min": 0.55,
                  "max": 2.35,
                  "neutral": 1.4
                },
                "angle": {
                  "min": 0,
                  "max": 18000,
                  "neutral": 9000
                }
              },
              "range": {
                "neutral": 9000,
                "max": 18000,
                "min": 0
              },
              "trim": 0.0
            },
            "focus": {
              "channel": -1,
              "type": "None",
              "attributes": {
                "pulse": {
                  "min": 0.55,
                  "max": 2.35,
                  "neutral": 1.4
                },
                "angle": {
                  "min": 0,
                  "max": 18000,
                  "neutral": 9000
                }
              },
              "range": {
                "neutral": 9000,
                "max": 18000,
                "min": 0
              },
              "trim": 0.0
            },
            "ircut": {
              "channel": -1,
              "type": "None",
              "attributes": {
                "pulse": {
                  "min": 0.55,
                  "max": 2.35,
                  "neutral": 1.4
                },
                "angle": {
                  "min": 0,
                  "max": 1,
                  "neutral": 0
                }
              },
              "range": {
                "neutral": 0,
                "max": 1,
                "min": 0
              },
              "trim": 0.0
            }
          }
        },
        "logging_level": "INFO",
        "az_logging_level": "WARNING",
        "period": 30,
        "powerdown": 30,
        "position": {
          "base": 0,
          "elevation": 68
        },
        "focus": 0,
        "zoom": 0,
        "ircut": false
      }
    }
  }
}
